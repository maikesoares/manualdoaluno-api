generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Informacao {
  id          Int     @id @default(autoincrement())
  title       String? @db.VarChar(100)
  description String? @db.VarChar(5000)

  @@map("informacoes")
}
